<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NoSuperUserExistsController">
   <children>
      <Label fx:id="PrefixedMessage2" layoutX="56.0" layoutY="239.0" prefHeight="27.0" prefWidth="489.0" text="Would you like to be the SuperUser of the SuperStore?">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="PrefixedMessage1" layoutX="56.0" layoutY="193.0" prefHeight="18.0" prefWidth="426.0" text="No SuperUser Exists.No other user exists as well.">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="YesButton" layoutX="286.0" layoutY="313.0" mnemonicParsing="false" text="Yes,Definitely">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="NoButton" layoutX="269.0" layoutY="389.0" mnemonicParsing="false" text="No,Count me Out">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="Quit" layoutX="517.0" layoutY="495.0" mnemonicParsing="false" text="Quit SuperStore">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ImageView fx:id="MainImage" fitHeight="150.0" fitWidth="396.0" layoutX="178.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
