            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="BrowseCategoryButton" layoutX="62.0" layoutY="206.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="212.0" text="Browse Category">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="366.0" layoutY="192.0" prefHeight="18.0" prefWidth="73.0" text="Search:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="SearchCategoryBox" layoutX="440.0" layoutY="193.0" prefHeight="26.0" prefWidth="265.0" promptText="Search any Category" />
   </children>
</AnchorPane>
