<AnchorPane fx:id="backbutton" prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StoreCategoryListController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="125.0" fitWidth="379.0" layoutX="232.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/SuperStore.png" />
         </image>
      </ImageView>
      <ListView fx:id="Categorylist" layoutX="366.0" layoutY="226.0" prefHeight="198.0" prefWidth="340.0" />
      <Button fx:id="AddCategoryButton" layoutX="62.0" layoutY="272.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="212.0" text="Add Category">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="DeleteCataegoryButton" layoutX="62.0" layoutY="334.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="212.0" text="Delete Category ">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="UpdateCategoryName" layoutX="62.0" layoutY="398.0" mnemonicParsing="false" text="Update Category Name">
         <font>
            <Font size="18.0" />
